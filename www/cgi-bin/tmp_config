config wifi-device 'radio0'
        option type 'mac80211'
        option channel 'auto'
        option hwmode '11g'
        option path 'platform/ar933x_wmac'
        option htmode 'HT20'
        option country 'US'

config wifi-iface
        option device 'radio0'
        option network 'WAN'
        option mode 'sta'
        option ssid 'default'	
        option encryption 'psk-mixed+tkip+ccmp'
        option key 'changeme'	
